<?xml version="1.0" encoding="UTF-8"?>
<phpunit bootstrap='tests/Math/bootstrap.php' colors='true'>
	<testsuites>
		<testsuite name='Math\Distance test suite'>
			<file phpVersion="5.3.0" phpVersionOperator=">=">tests/Math/DistanceTest.php</file>
		</testsuite>
	</testsuites>
	<filter>
		<blacklist>
			<directory suffix=".php">/usr/share/php</directory>
		</blacklist>
	</filter>
	<logging>
		<log type='coverage-html' target='local/report' charset='UTF-8'
			highlight="true" lowUpperBound="35" highLowerBound="70" />
		<log type='testdox-html' target='local/testdox.html' />
		<log type='tap' target='local/logfile.tap' />
	</logging>
</phpunit>
